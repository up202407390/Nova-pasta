   "source": [
    "import pandas as pd\n",
    "import sqlite3\n",
    "\n",
    "\n",
    "csv_file = '/mnt/data/G07_Library.csv'\n",
    "db_file = '/mnt/data/library.db'\n",
    "\n",
    "df = pd.read_csv(csv_file)\n",
    "print(df.head())\n",
    "\n",
    "conn = sqlite3.connect(db_file)\n",
    "cursor = conn.cursor()\n",
    "\n",
    "\n",
    "publishers = df[['publisher_name', 'country']].drop_duplicates()\n",
    "publishers.to_sql('Publisher', conn, if_exists='append', index=False)\n",
    "\n",
    "\n",
    "books = df[['books_id', 'title', 'author', 'genre', 'publisher_name']].drop_duplicates()\n",
    "books.to_sql('Book', conn, if_exists='append', index=False)\n",
    "\n",
    "\n",
    "members = df[['members_id', 'name', 'email', 'membership_type', 'country']].drop_duplicates()\n",
    "members.to_sql('Member', conn, if_exists='append', index=False)\n",
    "\n",
    "\n",
    "requisitions = df[['borrow_date', 'return_date', 'books_id', 'members_id']]\n",
    "requisitions.to_sql('Requisition', conn, if_exists='append', index=False)\n",
    "\n",
    "conn.close()"
   ]
  }
 ],
